<div class="index-section{% if section.settings.alt %} index-section--alt{% endif %}">

<div id="container">
      
      
<div id="slider-1">
    <ul class="slides">
      <li class="gallery-cell">
        {% if block.settings.image_1 != blank %}
        <img src="{{ block.settings.image_1 }}">
      </li>
      {% endif %}
      
      {% if block.settings.image_2 != blank %}
      <li class="gallery-cell">
		{%- assign img_url = block.settings.image_2 | img_url: '1x1' -%}
      </li>
      {% endif %}
      
      {% if block.settings.image_3 != blank %}
      <li class="gallery-cell">
		{%- assign img_url = block.settings.image_3 | img_url: '1x1' -%}
      </li>
      {% endif %}
    </ul>
  </div>
  
  
<div id="slider-2">
    <ul class="slides">
      {% if block.settings.image_4 != blank %}
      <li class="gallery-cell">
        {%- assign img_url = block.settings.image_4 | img_url: '1x1' -%}
      </li>
      {% endif %}
      
      {% if block.settings.image_5 != blank %}
      <li class="gallery-cell">
		{%- assign img_url = block.settings.image_5 | img_url: '1x1' -%}
      </li>
      {% endif %}
      
      {% if block.settings.image_6 != blank %}
      <li class="gallery-cell">
		{%- assign img_url = block.settings.image_6 | img_url: '1x1' -%}
      </li>
      {% endif %}
    </ul>
  </div>
  
  
  <button class="btn-next">
      <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 29.3 13.4">
        <path id="arrow-line" fill="#231F20" d="M0 5.9h28.7v1H0z"/>
        <path fill="#231F20" d="M24.1 13.4l-.8-.6 4.8-6.4L23.3 0l.8-.6 5.2 7"/>
      </svg>
    </button>
  
</div>
</div>



  

